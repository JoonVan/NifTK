<statemachine>
  <state name="start" startstate="true">
    <transition event_class="InteractionKeyEvent" event_variant="SelectSeedTool" target="start">
      <action name="selectSeedTool"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="SelectDrawTool" target="start">
      <action name="selectDrawTool"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="SelectPolyTool" target="start">
      <action name="selectPolyTool"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="UnselectTools" target="start">
      <action name="unselectTools"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="UnselectTool2" target="start">
      <action name="unselectTools"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="SelectViewMode" target="start">
      <action name="selectViewMode"/>
    </transition>
    <transition event_class="InteractionKeyEvent" event_variant="CleanSlice" target="start">
      <action name="cleanSlice"/>
    </transition>
  </state>
</statemachine>

SET(NifTK_LICENSE_NIFTYREC "@CMAKE_SOURCE_DIR@/Doc/Licenses/NiftyRec.txt")
SET(NifTK_LICENSE_NIFTYREG "@CMAKE_SOURCE_DIR@/Doc/Licenses/NiftyReg.txt")
SET(NifTK_LICENSE_NIFTYSIM "@CMAKE_SOURCE_DIR@/Doc/Licenses/NiftySim.txt")
SET(NifTK_LICENSE_NIFTYSEG "@CMAKE_SOURCE_DIR@/Doc/Licenses/NiftySeg.txt")
